              <input type="hidden" name="rm" value="groupList">
              <input type="hidden" name="type" value="property">
              Value of&nbsp;
              <select name="prop">
                 <option>Percentage Match Pairs (0..100)</option>
                 <option>Average BLAST E-value (0..1e-5)</option>
<!--                 <option>Average DCS (0..100)</option> -->
                 <option>Average Pencentage Identity (0..100)</option>
                 <option>Average Pencentage Match (0..100)</option>
              </select>
              <input type="text" size=10 name="number" value=">=10">
            </td>
            <td align="left"><input type="submit" value="GO!"></td>
